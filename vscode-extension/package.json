{
  "name": "parallels-mcp-extension",
  "displayName": "Parallels MCP Server",
  "description": "Run the Parallels MCP server from VS Code and manage Parallels Desktop VMs via MCP tools.",
  "version": "0.1.3",
  "publisher": "maslabs",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/masmedh/parallels-mcp-server.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "parallelsMcp.startServer",
        "title": "Parallels MCP: Start Server"
      },
      {
        "command": "parallelsMcp.stopServer",
        "title": "Parallels MCP: Stop Server"
      },
      {
        "command": "parallelsMcp.restartServer",
        "title": "Parallels MCP: Restart Server"
      },
      {
        "command": "parallelsMcp.serverStatus",
        "title": "Parallels MCP: Show Server Status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "dependencies": {},
  "bundledDependencies": [],
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.7.2",
    "@vscode/vsce": "^2.29.0"
  }
}
